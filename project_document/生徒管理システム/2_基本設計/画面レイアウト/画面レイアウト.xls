            sb.AppendLine(" GROUP BY v_customer_product_latest.product_id, t_product_setting.config_specification_id, m_delivery.delete_flg, t_product_setting.delete_flg, t_product_setting_detail.delete_flg, v_customer_product_latest.delete_flg ");
