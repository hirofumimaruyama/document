        #region 原料・産地設定詳細情報一覧を取得する
        /// <summary>
        /// 原料・産地設定詳細情報一覧を取得する
        /// </summary>
        /// <param name="CustomerId">顧客ID</param>
        /// <param name="DeliveryId">取引先ID</param>
        /// <param name="ProductId">商品コード</param>
        /// <param name="MaterialInfoId">原料・産地設定情報</param>
        /// <returns></returns>
        public BindingList<UseMaterialInfoProductSettinInfoDto> GetUseMaterialInfoProdutItemSetting(string CustomerId, string DeliveryId, string ProductId, string MaterialInfoId)
        {
            StringBuilder sb = new StringBuilder();
            List<OleDbParameter> parameterList = new List<OleDbParameter>();
            IsExistsWhere isExistsWhere = IsExistsWhere.Yes;

            #region SQL文
            sb.AppendLine(" SELECT");
            sb.AppendLine("  m_delivery.customer_id,");
            sb.AppendLine("  m_delivery.delivery_customer_id,");
            sb.AppendLine("  m_delivery.delivery_customer_name,");
            sb.AppendLine("  t_product_setting.product_id,");
            sb.AppendLine("  t_product_setting.product_cd,");
            sb.AppendLine("  m_product.product_name,");
            sb.AppendLine("  t_product_setting.config_specification_id,");
            sb.AppendLine("  t_product_setting_detail.branch_no,");
            sb.AppendLine("  t_product_setting_detail.material_info_id,");
            sb.AppendLine("  t_product_setting_detail.material_info_individual_id,");
            sb.AppendLine("  t_product_setting_detail.position,");
            sb.AppendLine("  t_product_setting_detail.remark,");
            sb.AppendLine("  t_product_setting_detail.delete_flg,");
            sb.AppendLine("  t_product_setting_detail.insert_datetime,");
            sb.AppendLine("  t_product_setting_detail.insert_user_id,");
            sb.AppendLine("  t_product_setting_detail.update_datetime,");
            sb.AppendLine("  t_product_setting_detail.update_user_id");
            sb.AppendLine(" FROM");
            sb.AppendLine("  (");
            sb.AppendLine("   (");
            sb.AppendLine("    m_customer");
            sb.AppendLine("    INNER JOIN m_delivery ON m_customer.customer_id = m_delivery.customer_id");
            sb.AppendLine("   )");
            sb.AppendLine("   INNER JOIN");
            sb.AppendLine("   (");
            sb.AppendLine("    (");
            sb.AppendLine("     m_product");
            sb.AppendLine("     INNER JOIN v_customer_product_latest ON m_product.product_id = v_customer_product_latest.product_id");
            sb.AppendLine("    )");
            sb.AppendLine("    INNER JOIN t_product_setting ON m_product.product_id = t_product_setting.product_id");
            sb.AppendLine("   )");
            sb.AppendLine("   ON (m_delivery.delivery_customer_id = v_customer_product_latest.delivery_customer_id) AND (m_delivery.customer_id = v_customer_product_latest.customer_id)");
            sb.AppendLine("  )");
            sb.AppendLine("  INNER JOIN");
            sb.AppendLine("  (");
            sb.AppendLine("   (");
            sb.AppendLine("    t_material_info");
            sb.AppendLine("    INNER JOIN t_material_info_detail ON t_material_info.material_info_id = t_material_info_detail.material_info_id");
            sb.AppendLine("   )");
            sb.AppendLine("   INNER JOIN t_product_setting_detail ON t_material_info.material_info_id = t_product_setting_detail.material_info_id");
            sb.AppendLine("  )");
            sb.AppendLine("  ON (t_product_setting.product_id = t_product_setting_detail.product_id) AND (t_product_setting.config_specification_id = t_product_setting_detail.config_specification_id)");
            sb.AppendLine(" WHERE");
            sb.AppendLine(DataAccessManager.MASK_TEXT_WHERE1);
            sb.AppendLine(" GROUP BY");
            sb.AppendLine("  m_delivery.customer_id,");
            sb.AppendLine("  m_delivery.delivery_customer_id,");
            sb.AppendLine("  m_delivery.delivery_customer_name,");
            sb.AppendLine("  t_product_setting.product_id,");
            sb.AppendLine("  t_product_setting.product_cd,");
            sb.AppendLine("  m_product.product_name,");
            sb.AppendLine("  t_product_setting.config_specification_id,");
            sb.AppendLine("  t_product_setting_detail.branch_no,");
            sb.AppendLine("  t_product_setting_detail.material_info_id,");
            sb.AppendLine("  t_product_setting_detail.material_info_individual_id,");
            sb.AppendLine("  t_product_setting_detail.position,");
            sb.AppendLine("  t_product_setting_detail.remark,");
            sb.AppendLine("  t_product_setting_detail.delete_flg,");
            sb.AppendLine("  t_product_setting_detail.insert_datetime,");
            sb.AppendLine("  t_product_setting_detail.insert_user_id,");
            sb.AppendLine("  t_product_setting_detail.update_datetime,");
            sb.AppendLine("  t_product_setting_detail.update_user_id");
            sb.AppendLine(" ORDER BY");
            sb.AppendLine("  m_delivery.customer_id,");
            sb.AppendLine("  m_delivery.delivery_customer_id,");
            sb.AppendLine("  t_product_setting.product_id,");
            sb.AppendLine("  t_product_setting.config_specification_id,");
            sb.AppendLine("  t_product_setting_detail.branch_no,");
            sb.AppendLine("  t_product_setting_detail.material_info_id,");
            sb.AppendLine("  t_product_setting_detail.material_info_individual_id;");

            #endregion SQL文

            // 追加条件カウント用変数
            int addCount = 0;

            // SQLに付加するWHERE条件文格納変数
            string where = string.Empty;

            // 条件生成DTOクラスのインスタンスを生成
            QueryConditionDto queryConditionDto = null;

            if (!CommonUtil.IsNullOrEmpty(CustomerId))
            {
                queryConditionDto = new QueryConditionDto
                    (DbObjectNameConst.TableName.M_CUSTOMER,
                    m_delivery.CUSTOMER_ID,
                    CustomerId,
                    QueryConditionDto.JoinType.And,
                    SearcherConst.MatchType.Perfect,
                    isExistsWhere,
                    addCount);
                where += queryConditionDto.Query;
                parameterList.Add(queryConditionDto.Parameter);
                addCount++;
            }

            if (!CommonUtil.IsNullOrEmpty(DeliveryId))
            {
                queryConditionDto = new QueryConditionDto
                    (DbObjectNameConst.TableName.M_DELIVERY,
                    m_delivery.DELIVERY_CUSTOMER_ID,
                    DeliveryId,
                    QueryConditionDto.JoinType.And,
                    SearcherConst.MatchType.Perfect,
                    isExistsWhere,
                    addCount);
                where += queryConditionDto.Query;
                parameterList.Add(queryConditionDto.Parameter);
                addCount++;
            }

            if (!CommonUtil.IsNullOrEmpty(ProductId))
            {
                queryConditionDto = new QueryConditionDto
                    (DbObjectNameConst.TableName.M_PRODUCT,
                    m_product.PRODUCT_ID,
                    ProductId,
                    QueryConditionDto.JoinType.And,
                    SearcherConst.MatchType.Perfect,
                    isExistsWhere,
                    addCount);
                where += queryConditionDto.Query;
                parameterList.Add(queryConditionDto.Parameter);
                addCount++;
            }

            if (CommonUtil.IsNullOrEmpty(MaterialInfoId))
            {
                queryConditionDto = new QueryConditionDto
                    (DbObjectNameConst.TableName.T_MATERIAL_INFO_DETAIL,
                    t_material_info_detail.MATERIAL_INFO_ID,
                    MaterialInfoId,
                    QueryConditionDto.JoinType.And,
                    SearcherConst.MatchType.Perfect,
                    isExistsWhere,
                    addCount);
                where += queryConditionDto.Query;
                parameterList.Add(queryConditionDto.Parameter);
                addCount++;
            }

            // 発行するSQL文を生成する
            string sql = sb.ToString().Replace(DataAccessManager.MASK_TEXT_WHERE1, where);
            // データアクセスクラスのインスタンスを生成する
            DataAccessManager databaseAccessMng = new DataAccessManager(AppConfigConst.DB_CONNECTION_STRING);
            // SQLを発行するし、戻り値を取得する
            BindingList<UseMaterialInfoProductSettinInfoDto> bindingList = this.ConvertUseMaterialInfoProductSettinInfoDtoToDataTable(databaseAccessMng.GetData(sql, parameterList));

            return bindingList;
        }
        #endregion 原料・産地設定詳細情報一覧を取得する

        #region 商品情報管理画面：顧客ベースツリービュー　第一ノードに表示する情報を取得する
        /// <summary>
        /// 商品情報管理画面：顧客ベースツリービュー　第一ノードに表示する情報を取得する
        /// </summary>
        /// <param name="CustomerId"></param>
        /// <param name="DeliveryId"></param>
        /// <param name="DeliveryNameKanaFirst"></param>
        /// <param name="ProductId"></param>
        /// <param name="SearchCondition"></param>
        /// <returns></returns>
        public BindingList<DeliveryInfoTreeViewDto> GetCustomerProductFirstNodeDtoList(string CustomerId, string DeliveryId, string DeliveryNameKanaFirst, string ProductId, SearchCustomerInfoDto SearchCondition)
        {
            StringBuilder sb = new StringBuilder();
            string where1 = string.Empty;

            string where2 = string.Empty;
            #region SELECT文を生成する
            sb.AppendLine(" SELECT work2.delivery_customer_name_kana_first, Count(work2.delivery_customer_id) as record_count");
            sb.AppendLine(" FROM (SELECT work.customer_id, work.delivery_customer_id, work.delivery_customer_name_kana_first, work.delivery_customer_name_kana_w, work.delivery_customer_name, work.product_id");
            sb.AppendLine(" FROM [SELECT m_customer.customer_id, m_delivery.delivery_customer_id, m_delivery.delivery_customer_name_kana_first, m_delivery.delivery_customer_name_kana_w, m_delivery.delivery_customer_name, v_customer_product_latest.product_id");
            sb.AppendLine(" FROM ((m_customer INNER JOIN m_delivery ON m_customer.customer_id = m_delivery.customer_id) INNER JOIN (m_product INNER JOIN v_customer_product_latest ON m_product.product_id = v_customer_product_latest.product_id) ON (m_delivery.customer_id = v_customer_product_latest.customer_id) AND (m_delivery.delivery_customer_id = v_customer_product_latest.delivery_customer_id)) INNER JOIN (t_product_setting INNER JOIN ((m_material_origin INNER JOIN (m_material INNER JOIN (t_material_info INNER JOIN t_material_info_detail ON t_material_info.material_info_id = t_material_info_detail.material_info_id) ON m_material.material_id = t_material_info_detail.material_id) ON m_material_origin.origin_id = t_material_info_detail.origin_id) INNER JOIN t_product_setting_detail ON t_material_info.material_info_id = t_product_setting_detail.material_info_id) ON (t_product_setting.product_id = t_product_setting_detail.product_id) AND (t_product_setting.config_specification_id = t_product_setting_detail.config_specification_id)) ON (v_customer_product_latest.product_id = t_product_setting.product_id) AND (v_customer_product_latest.config_specification_id = t_product_setting.config_specification_id)");
            sb.AppendLine(" WHERE");
            sb.AppendLine(DataAccessManager.MASK_TEXT_WHERE1);
            sb.AppendLine(" union all");
            sb.AppendLine(" SELECT m_customer.customer_id, m_delivery.delivery_customer_id, m_delivery.delivery_customer_name_kana_first, m_delivery.delivery_customer_name_kana_w, m_delivery.delivery_customer_name, '' AS product_id");
            sb.AppendLine(" FROM m_customer INNER JOIN m_delivery ON m_customer.customer_id = m_delivery.customer_id");
            sb.AppendLine(" WHERE");
            sb.AppendLine(DataAccessManager.MASK_TEXT_WHERE1);
            sb.AppendLine("]. AS [work]"); 
            sb.AppendLine(" GROUP BY work.customer_id, work.delivery_customer_id, work.delivery_customer_name_kana_first, work.delivery_customer_name_kana_w,  work.delivery_customer_name, work.product_id");
            sb.AppendLine(" ) work2");
            sb.AppendLine(DataAccessManager.MASK_TEXT_WHERE2);
            sb.AppendLine(" GROUP BY work2.delivery_customer_name_kana_first");
            sb.AppendLine(" ORDER BY work2.delivery_customer_name_kana_first");
            #endregion SELECT文を生成する

            QueryConditionDto queryConditionDto = null;
            addCount = 0;
            List<OleDbParameter> parameterList = new List<OleDbParameter>();
            IsExistsWhere isExistsWhere = IsExistsWhere.No;

            if (!CommonUtil.IsNullOrEmpty(CustomerId))
            {
                {
                    queryConditionDto = new QueryConditionDto
                        (
                        "work2",
                        m_customer.CUSTOMER_ID,
                        CustomerId,
                        QueryConditionDto.JoinType.And,
                        SearcherConst.MatchType.Perfect,
                        isExistsWhere,
                        addCount);
                    where2 += queryConditionDto.Query;
                    parameterList.Add(queryConditionDto.Parameter);
                    addCount++;
                }
            }

            if(!CommonUtil.IsNullOrEmpty(DeliveryId))
            {
                queryConditionDto = new QueryConditionDto
                    (
                    "work2",
                    m_delivery.DELIVERY_CUSTOMER_ID,
                    DeliveryId,
                    QueryConditionDto.JoinType.And,
                    SearcherConst.MatchType.Perfect,
                    isExistsWhere,
                    addCount);
                where2 += queryConditionDto.Query;
                parameterList.Add(queryConditionDto.Parameter);
                addCount++;
            }

            if (!CommonUtil.IsNullOrEmpty(DeliveryNameKanaFirst))
            {
                queryConditionDto = new QueryConditionDto
                    (
                    "work2",
                    m_delivery.DELIVERY_CUSTOMER_NAME_FIRST,
                    DeliveryNameKanaFirst,
                    QueryConditionDto.JoinType.And,
                    SearcherConst.MatchType.Perfect,
                    isExistsWhere,
                    addCount);
                where2 += queryConditionDto.Query;
                parameterList.Add(queryConditionDto.Parameter);
                addCount++;
            }

            if (!CommonUtil.IsNullOrEmpty(ProductId))
            {
                queryConditionDto = new QueryConditionDto
                    (
                    "work2",
                    m_product.PRODUCT_ID,
                    DeliveryNameKanaFirst,
                    QueryConditionDto.JoinType.And,
                    SearcherConst.MatchType.Perfect,
                    isExistsWhere,
                    addCount);
                where2 += queryConditionDto.Query;
                parameterList.Add(queryConditionDto.Parameter);
                addCount++;
            }

            // 削除情報の検索方法
            string deleteInfo = base.GetDeleteInfo(SearchCondition.DeleteInfo);

            if (!CommonUtil.IsNullOrEmpty(deleteInfo))
            {
                where1 += " (" + DbObjectNameConst.TableName.M_DELIVERY + "." + deleteInfo + ")";
            }

            string sql = sb.ToString();
            sql = sql.Replace(DataAccessManager.MASK_TEXT_WHERE1, where1);
            sql = sql.Replace(DataAccessManager.MASK_TEXT_WHERE2, where2);


            // データアクセスクラスのインスタンスを生成する
            DataAccessManager databaseAccessMng = new DataAccessManager(AppConfigConst.DB_CONNECTION_STRING);
            return this.ConvertDataTableToDeliveryInfoTreeViewDto(databaseAccessMng.GetData(sql, parameterList));
        }
        #endregion 商品情報管理画面：顧客ベースツリービュー　第一ノードに表示する情報を取得する

        #region 商品情報管理画面：顧客ベースツリービュー　第二ノードに表示する情報を取得する
        /// <summary>
        /// 商品情報管理画面：顧客ベースツリービュー　第二ノードに表示する情報を取得する
        /// </summary>
        /// <param name="CustomerId"></param>
        /// <param name="DeliveryId"></param>
        /// <param name="DeliveryNameKanaFirst"></param>
        /// <param name="ProductId"></param>
        /// <param name="SearchCondition"></param>
        /// <returns></returns>
        public BindingList<CustomerItemRecordCountDto> GetCustomerProductSecondNodeDtoList(string CustomerId, string DeliveryId, string DeliveryNameKanaFirst, string ProductId, SearchCustomerInfoDto SearchCondition)
